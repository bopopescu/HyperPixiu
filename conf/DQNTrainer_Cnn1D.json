// configuration of a DQNTrainer
{
    "logger": {
        "level": "info", //40
        // "console": "False",
    },

    "DQNTrainer": {

        "modelId": "Cnn1Dx4R1", // default "Cnn1Dx4"
        // "replayFrameFiles" : ["/mnt/e/AShareSample/ETF/RFrames_SH510050.h5"],
        "replayFrameFiles" : ["/mnt/e/AShareSample/ETF/RFrames_SH510300.h5"],

        "batchSize": 64,
        "batchesPerTrain": 32,
        "poolReuses": 0,
        "initEpochs": 2,
        "startLR": 0.001, // start learningRate

        "GPU": {
            "batchSize": 128,
            // "startLR": 0.005,
            "batchesPerTrain": 128, // 64 for GTX1050/2G, 128 for GTX1060/4G
            "poolReuses": 0,
            "initEpochs": 8,
        },

        "gamma": 0.01,
    },
}
