// configuration of a DQNTrainer
{
    "logger": {
        "level": "info", //40
        // "console": "False",
    },

    "DQNTrainer": {

        "modelId": "Cnn1Dx4R2", // default "Cnn1Dx4"
        // "replayFrameFiles" : ["/mnt/e/AShareSample/ETF/RFrames_SH510050.h5"],
        // "replayFrameFiles" : ["/mnt/e/AShareSample/ETF/RFrames_SH510300.h5"],
        "replayFrameDir": "/mnt/e/AShareSample/500top50",

        "batchSize": 64,
        "batchesPerTrain": 32,
        "poolReuses": 0,
        "initEpochs": 2,
        "startLR": 0.002, // start learningRate

        "GPU": {
            "batchSize": 128,
            "batchesPerTrain": 128, // 512x64 for GTX1050/2G, 512x128 for GTX1060/4G
            "recycles": 2,
            "initEpochs": 4,
            "startLR": 0.002,
            // "lossPctStop": 50,
            "lossStop": 0.15,
        },

        "RTX2080": {
            "batchSize": 512,
            "tensorBoard": "yes",
            "batchesPerTrain": 192,
            "recycles": 2,
            "initEpochs": 8, // 8 when the model is not well trained, 4 when trained and try to cover larger dataset
        },

        "gamma": 0.01,
    },
}
